{
  "name": "pdf-minimizer",
  "version": "1.0.0",
  "description": "Minimizes PDFs",
  "scripts": {
    "dev": "ts-node-dev --respawn --clear --rs --pretty --inspect -- ./index.ts",
    "build": "esbuild ./index.ts --bundle --platform=node --outfile=index.js",
    "start": "node index.js"
  },
  "main": "index.js",
  "author": "Josip Igrec <josip@igr.ec>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "multer": "^1.4.4"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.23",
    "esbuild": "^0.14.31",
    "ts-node-dev": "^1.1.8",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  }
}
